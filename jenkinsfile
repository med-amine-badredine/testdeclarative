pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                echo 'getting from github'
                echo 'checkout completed'
            }
        }
        
        stage('build') {
            steps {
                echo 'build completed'
            }
        }
        
        stage('test') {
            steps {
                echo 'test completed'
            }
        }
        
        stage('deploy') {
            steps {
                echo 'deploy completed'
            }
        }
    }
}
